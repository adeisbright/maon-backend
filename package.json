{
  "name": "maon-backend-challenge",
  "version": "1.0.0",
  "description": "A Stackoverflow clone with endpoints for creating questions , searching for response , etc",
  "main": "app.js",
  "scripts": {
    "unittest": "mocha ./test/unit --timeout 10000",
    "integrationtest": "mocha ./test/integration --timeout 10000",
    "test": "mocha ./test/unit && ./test/integration --timeout 10000",
    "devstart": "nodemon ./bin/www",
    "start": "node ./bin/www",
    "unitcoverage": "nyc npm run unittest",
    "integrationcoverage": "nyc npm run integrationtest",
    "lint": "eslint .",
    "lint-fix": "npm run lint -- --fix",
    "pretest": "npm run lint",
    "letest": "mocha ./src/services/*/*.test.js"
  },
  "keywords": [
    "Maontech backend challenge",
    "RESTFUL API",
    "API Design",
    "Stackoverflow Clone"
  ],
  "author": "Adeleke Bright",
  "license": "ISC",
  "dependencies": {
    "@elastic/elasticsearch": "^7.16.0",
    "@google-cloud/storage": "^5.15.3",
    "@sendgrid/mail": "^7.5.0",
    "amqplib": "^0.8.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "dotenv": "^10.0.0",
    "elasticsearch": "^16.7.2",
    "express": "^4.17.1",
    "flutterwave-node-v3": "^1.0.9",
    "googleapis": "^89.0.0",
    "graphql": "^16.0.1",
    "helmet": "^4.6.0",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "kafkajs": "^1.15.0",
    "mongoose": "^6.0.12",
